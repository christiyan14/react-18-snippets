{
  "name": "react-18-snippets",
  "displayName": "React 18 Snippets",
  "description": "React 18 snippets for functional components you will actually use",
  "version": "1.0.0",
  "publisher": "kristiyanvelkov",
  "icon": "images/react-logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/christiyan14/react-18-snippets.git"
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "test": "node ./node_modules/@vscode/test-electron/dist/runTest.js"
  },
  "keywords": [
    "React",
    "React 18",
    "React 18 Snippets",
    "React JS",
    "React Snippets"
  ],
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:extension.stayClassy"
  ],
  "main": "./dist/src/extension",
  "browser": "./dist/src/extension",
  "contributes": {
    "snippets": [
      {
        "language": "javascriptreact",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/snippets-ts.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/snippets-ts.json"
      }
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "@types/vscode": "^1.63.0",
    "@vscode/test-electron": "^1.6.1",
    "mocha": "^8.0.1",
    "typescript": "^4.0.0"
  }
}